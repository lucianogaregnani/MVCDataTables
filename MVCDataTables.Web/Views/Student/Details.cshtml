@model MVCDataTables.Domain.Entities.Student

<h1>Details</h1>

<div>
	<h4>Student</h4>
	<hr />
	<dl class="row">
		<dt class="col-sm-2">
			Id
		</dt>
		<dd class="col-sm-10">
			@Model.Id
		</dd>
		<dt class="col-sm-2">
			Name
		</dt>
		<dd class="col-sm-10">
			@Model.Name
		</dd>
		<dt class="col-sm-2">
			Age
		</dt>
		<dd class="col-sm-10">
			@Model.Age
		</dd>
		<dt class="col-sm-2">
			Country
		</dt>
		<dd class="col-sm-10">
			@Model.Country
		</dd>
	</dl>
	<a asp-controller="Student" asp-action="Index" class="btn btn-secondary">Volver</a>
</div>